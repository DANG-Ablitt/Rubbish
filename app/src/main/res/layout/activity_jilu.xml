<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/addressmanager_total_layout"
    android:background="@color/white1"
    android:paddingTop="5dp" >
    <RelativeLayout
        android:id="@+id/addressmanager_layout"
        android:padding="1dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white1">
        <ImageView
            android:clickable="true"
            android:layout_width="20dp"
            android:layout_height="25dp"
            android:id="@+id/addressmanager_back_imageview"
            android:src="@drawable/fanhui"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/grey"
            android:text="记录"
            android:textSize="18sp"
            android:layout_centerInParent="true"/>

    </RelativeLayout>
    <ImageView
        android:id="@+id/addressmanager_xian"
        style="@style/bankcard_xian"
        android:layout_marginTop="5dp"
        android:layout_below="@+id/addressmanager_layout"/>
    <ListView
        android:layout_marginTop="10dp"
        android:layout_below="@+id/addressmanager_xian"
        android:id="@+id/address_list_address"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </ListView>

</RelativeLayout>
